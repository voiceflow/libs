{
  "name": "@voiceflow/base-types",
  "description": "Voiceflow base project types",
  "version": "2.50.0",
  "author": "Voiceflow",
  "bugs": {
    "url": "https://github.com/voiceflow/libs/issues"
  },
  "dependencies": {
    "@voiceflow/common": "^7.28.0",
    "slate": "0.73.0"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/voiceflow/libs#readme",
  "keywords": [
    "voiceflow",
    "voiceflow-types"
  ],
  "license": "ISC",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "publishConfig": {
    "access": "public",
    "types": "build/cjs/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/libs.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "ttsc --build tsconfig.build.json",
    "build:esm": "ttsc --build tsconfig.esm.json",
    "clean": "rimraf build",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "eslint-output --quiet '**/*.{js,ts}'",
    "test": "exit 0",
    "test:dependencies": "depcheck",
    "test:integration": "exit 0",
    "test:smoke": "exit 0",
    "test:unit": "exit 0"
  },
  "types": "src/index.ts"
}
