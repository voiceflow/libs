{
  "name": "@voiceflow/chat-types",
  "description": "Voiceflow chat project types",
  "version": "2.8.18",
  "author": "Voiceflow",
  "bugs": {
    "url": "https://github.com/voiceflow/libs/issues"
  },
  "dependencies": {
    "@voiceflow/base-types": "2.48.0",
    "@voiceflow/common": "7.27.3"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/voiceflow/libs#readme",
  "keywords": [
    "voiceflow",
    "voiceflow-types"
  ],
  "license": "ISC",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "publishConfig": {
    "access": "public",
    "types": "build/cjs/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/libs.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "ttsc --build tsconfig.build.json",
    "build:esm": "ttsc --build tsconfig.esm.json",
    "clean": "rimraf build",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint --format json -o sonar/report.json",
    "test": "exit 0",
    "test:dependencies": "depcheck",
    "test:integration": "exit 0",
    "test:unit": "exit 0"
  },
  "types": "src/index.ts"
}
