{
  "name": "@voiceflow/common",
  "description": "Junk drawer of utility functions",
  "version": "8.2.4",
  "author": "Voiceflow",
  "bugs": {
    "url": "https://github.com/voiceflow/libs/issues"
  },
  "dependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/shallowequal": "^1.1.1",
    "bson-objectid": "^2.0.1",
    "crypto-js": "^4.2.0",
    "cuid": "^2.1.8",
    "dayjs": "1.10.7",
    "lodash": "^4.17.21",
    "murmurhash-wasm": "^1.3.0",
    "number-to-words": "^1.2.4",
    "shallowequal": "^1.1.0",
    "typescript-fsa": "3.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.176",
    "@types/number-to-words": "1.2.0"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/voiceflow/libs#readme",
  "keywords": [
    "voiceflow"
  ],
  "license": "ISC",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "publishConfig": {
    "access": "public",
    "types": "build/cjs/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/libs.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "yarn g:tsc -p tsconfig.build.json && yarn g:tsc-alias -p tsconfig.build.json && yarn postbuild:cjs",
    "build:esm": "yarn g:tsc -p tsconfig.esm.json && yarn g:tsc-alias -p tsconfig.esm.json && yarn postbuild:esm",
    "clean": "yarn g:rimraf build",
    "commit": "cz",
    "lint": "yarn g:eslint '**/*.{js,ts}'",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn g:eslint-output --quiet '**/*.{js,ts}'",
    "postbuild:cjs": "touch build/cjs/package.json && echo '{ \"type\": \"commonjs\" }' > build/cjs/package.json",
    "postbuild:esm": "touch build/esm/package.json && echo '{ \"type\": \"module\" }' > build/esm/package.json",
    "tdd": "yarn test:unit --watch",
    "test:dependencies": "yarn g:depcheck",
    "test:unit": "NODE_ENV=test yarn g:nyc --report-dir=nyc_coverage_unit ts-mocha --paths --config config/tests/mocharc.yml 'tests/**/*.unit.ts'"
  },
  "types": "build/cjs/index.d.ts"
}
