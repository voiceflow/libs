{
  "name": "@voiceflow/api-sdk",
  "description": "wrapper for creator-api",
  "version": "3.27.17",
  "author": "Voiceflow",
  "bugs": {
    "url": "https://github.com/voiceflow/libs/issues"
  },
  "dependencies": {
    "@voiceflow/base-types": "workspace:*",
    "@voiceflow/common": "workspace:*",
    "axios": "0.24.0",
    "jwt-decode": "^3.1.2",
    "superstruct": "^0.10.12"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^18.11.11",
    "jsonwebtoken": "^8.5.1"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/voiceflow/libs#readme",
  "keywords": [
    "voiceflow"
  ],
  "license": "ISC",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "publishConfig": {
    "access": "public",
    "types": "build/cjs/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/libs.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "yarn g:tsc -p tsconfig.build.json && yarn g:tsc-alias -p tsconfig.build.json && yarn postbuild:cjs",
    "build:esm": "yarn g:tsc -p tsconfig.esm.json && yarn g:tsc-alias -p tsconfig.esm.json && yarn postbuild:esm",
    "clean": "yarn g:rimraf build",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "eslint-output --quiet '**/*.{js,ts}'",
    "postbuild:cjs": "touch build/cjs/package.json && echo '{ \"type\": \"commonjs\" }' > build/cjs/package.json",
    "postbuild:esm": "touch build/esm/package.json && echo '{ \"type\": \"module\" }' > build/esm/package.json",
    "tdd": "yarn test:unit --watch",
    "test:dependencies": "depcheck",
    "test:unit": "NODE_ENV=test nyc --report-dir=nyc_coverage_unit ts-mocha --paths --config ./config/tests/.mocharc.json 'tests/**/*.unit.ts'"
  },
  "types": "build/cjs/index.d.ts"
}
