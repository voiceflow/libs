{
  "name": "@voiceflow/voice-types",
  "description": "Voiceflow voice project types",
  "version": "2.9.70",
  "author": "Voiceflow",
  "bugs": {
    "url": "https://github.com/voiceflow/libs/issues"
  },
  "dependencies": {
    "@voiceflow/base-types": "^2.100.0",
    "@voiceflow/common": "^8.2.4"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/voiceflow/libs#readme",
  "keywords": [
    "voiceflow",
    "voiceflow-types"
  ],
  "license": "ISC",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "publishConfig": {
    "access": "public",
    "types": "build/cjs/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/libs.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && yarn postbuild:cjs",
    "build:esm": "tsc -p tsconfig.esm.json && tsc-alias -p tsconfig.esm.json && yarn postbuild:esm",
    "clean": "rimraf build",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "eslint-output --quiet '**/*.{js,ts}'",
    "postbuild:cjs": "touch build/cjs/package.json && echo '{ \"type\": \"commonjs\" }' > build/cjs/package.json",
    "postbuild:esm": "touch build/esm/package.json && echo '{ \"type\": \"module\" }' > build/esm/package.json",
    "test": "exit 0",
    "test:dependencies": "depcheck",
    "test:integration": "exit 0",
    "test:unit": "exit 0"
  },
  "types": "build/cjs/index.d.ts"
}
